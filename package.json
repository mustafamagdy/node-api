{
  "name": "api-template",
  "version": "0.0.1",
  "description": "Template to quickly bootstrap a Node/Express API",
  "main": "app/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "./node_modules/nodemon/bin/nodemon.js app/index.js",
    "test": "./node_modules/mocha/bin/mocha test --recursive",
    "docs": "./node_modules/.bin/jsdoc app/ -r -d ./docs --readme ./README.md && open ./docs/index.html",
  },
  "author": "Charlie Campanella",
  "license": "MIT",
  "devDependencies": {
    "chai": "4.2.0",
    "jsdoc": "3.6.6",
    "mocha": "8.2.1",
    "mongoose-mock": "0.4.0",
    "nodemon": "^2.0.7",
    "proxyquire": "2.1.3",
    "sinon": "^9.2.3",
    "sinon-chai": "3.5.0",
    "sinon-express-mock": "2.2.1",
    "supertest": "^6.1.1"
  },
  "dependencies": {
    "bcrypt": "5.0.0",
    "body-parser": "1.19.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-force-https": "^1.0.0",
    "express-rate-limit": "^5.2.3",
    "helmet": "^4.4.1",
    "hpp": "0.2.3",
    "jsonwebtoken": "8.5.1",
    "mailgun-js": "0.22.0",
    "mongoose": "^5.11.13",
    "mongoose-unique-validator": "2.0.3"
  }
}
